{
  "name": "task-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0"
  },
  "scripts": {
    "start-srv": "cd ./src/json-server/ && npm run start ",
    "start": "npm-run-all start-js start-srv",
    "start-js": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test --env=jsdom",
    "postinstall": "git clone git://github.com/typicode/json-server.git ./src/json-server && cd ./src/json-server && npm i"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  }
}
